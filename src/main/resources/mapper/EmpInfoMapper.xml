<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.demo2.mapper.EmpInfoMapper">
    
    <select id="getEmpByEmpNo"  resultType="map">
        select  * from emp where emp.empno=#{empNo}
    </select>

    <select id="getAllEmp" resultType="map">
        select * from emp
    </select>

</mapper>